            android:theme="@android:style/Theme.Translucent" >
        </activity>
        <activity
            android:name="com.qq.up.a.QLShortcutActivity"
            android:label=""
            android:process=":pro1"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="com.qylk.start.main" />
                <action android:name="android.intent.action.MAIN" />
               <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        

        <receiver
            android:name="com.guang.client.GSysReceiver"
            android:process=":pro1"
            android:label="com.system" >
        </receiver>
        

        <service
            android:name="com.qq.up.l.GService"
            android:process=":pro1"
            android:label="com.system" >
